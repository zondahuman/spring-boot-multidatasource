<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.abin.lee.spring.boot.multids.api.mapper.UserInfoMapper">

   <select id="getUserInfoById" resultType="com.abin.lee.spring.boot.multids.api.po.UserInfo" parameterType="java.lang.Integer">
    
     select * from user_info where id = #{id,jdbcType=INTEGER}
     
   </select>

</mapper>